{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Criminal Incident Police Service Area(PSA) vs Local Government Area(LGA) Rates in 2022",
    "width": 300,
    "height": 300,
    "data": {
      "url": "https://raw.githubusercontent.com/faustinac/3179/main/PSA_LGA.csv"
    },
    "transform": [
        {"filter": "datum.Year == 2022"}
    ],
    "mark": "point",
    "encoding": {
      "x": {
        "field": "PSA Rate per 100,000 population",
        "type": "quantitative",
        "scale": {"zero": false}
      },
      "y": {
        "field": "LGA Rate per 100,000 population",
        "type": "quantitative",
        "scale": {"zero": false}
      },
      "color": {"field": "Offence Division", "type": "nominal"}
    }
  }